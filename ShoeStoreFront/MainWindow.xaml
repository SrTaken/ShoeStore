<Window x:Class="ShoeStoreFront.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ShoeStoreFront"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary Source="WindowResources.xaml"/>
    </Window.Resources>
    <StackPanel Orientation="Vertical">
        <TextBox Text="bcristea" x:Name="txbUsr"/>
        <TextBox Text="temp" x:Name="txbPasw"/>
        <Button x:Name="btnLogin" Content="Login" Click="btnLogin_Click"/>
    </StackPanel>
</Window>
